SetHabillage = function() {


if (habillage_state	== 2)
	return 0;

console.log('hs '+habillage_state);
habillage_state = 1;	
var contents = $('#google_ads_iframe_\\/1032445\\/habillage_site_0').contents();
var CLICK_URL_UNESC = contents.find('#CLICK_URL_UNESC').val();

if (CLICK_URL_UNESC !='' && CLICK_URL_UNESC !== undefined )
{
habillage_state = 2;

var clickthrougurl = contents.find('#clickthrougurl').val();
var Targetwindow = contents.find('#Targetwindow').val();
var VIEW_URL_UNESC = contents.find('#VIEW_URL_UNESC').val();
var backgroundimage = contents.find('#backgroundimage').val();
var Couleurdefond = contents.find('#Couleurdefond').val();
var PATTERNhabillage = contents.find('#PATTERNhabillage').val();
	
if (PATTERNhabillage =='2')
    $(window.parent.document).find('body').css({
        "background-attachment": "scroll",
        "background-image": "url('"+backgroundimage+"')",
        "background-position": "center 40px",
        "background-color": Couleurdefond,
        "background-repeat": "no-repeat"
    });
else
    $(window.parent.document).find('body').css({
        "background-attachment": "scroll",
        "background-image": "url('"+backgroundimage+"')",
        "background-position": "top center",
        "background-color": Couleurdefond,
        "background-repeat": "no-repeat"
    });


   

   // $("#bandeau_incitation").hide();

//v1
//22 px image brise 41 hauteur bande incit 5 padding-bottom barre
//190+22-41+5=209
if (PATTERNhabillage =='2')
  $("#wrapper").css({"margin-top" : "204px"});
else
  $("#wrapper").css({"margin-top" : "168px"});


    $(window.parent.document).find('body')
        .after('<a href="'+CLICK_URL_UNESC+clickthrougurl+'" target="_'+Targetwindow+'">' + 
            '<img src="'+VIEW_URL_UNESC+'"  style="display:block;position:fixed;left:0;top:0;width:100%;height:100%;cursor:pointer" /></a>');
}
}


 $('#google_ads_iframe_\\/1032445\\/habillage_site_0').load(SetHabillage );
 

