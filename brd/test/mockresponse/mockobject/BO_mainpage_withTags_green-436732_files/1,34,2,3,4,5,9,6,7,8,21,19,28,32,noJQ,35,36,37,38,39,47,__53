
var timer=null;var IE6=false;var strChUserAgent=navigator.userAgent;var intSplitStart=strChUserAgent.indexOf("(",0);var intSplitEnd=strChUserAgent.indexOf(")",0);var strChMid=strChUserAgent.substring(intSplitStart,intSplitEnd);var isIE=(navigator.appName.indexOf("Microsoft")!=-1)?1:0;function addLoadEvent(func)
