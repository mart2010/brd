{oldonload();func();}}}
function show(id)
{obj=document.getElementById(id);obj.style.display=obj.style.display==""||obj.style.display=="block"?"none":"";}
function show22(id)
{obj=document.getElementById(id);switch(obj.className)
{case"MOnglet":obj.className="MOnglet_on";break;case"MOnglet_on":obj.className="MOnglet";break;}}
function show_form(formId,target)
{timer=setTimeout("chrono()","20");objForm=document.getElementById(formId);objForm.style.left=target.offsetLeft-15+"px";objForm.style.top=target.offsetTop+15+"px";objForm.style.display="";}
function hide_form(formId)
{var objForm=document.getElementById(formId);if(objForm)
{objForm.style.display="none";}}
function showMenu(obj)
{switch(obj.className)
{case"active":case"active_on":break;default:obj.className=obj.className=="on"?"":"on";break;}}
function roll(target,nTarget,obj,action)
{for(i=1;i<nTarget+1;i++)
{obj2=document.getElementById(target+i);obj2.className=(obj2.className=="active"||obj2.className=="active_off")?"active_off":"";}
if(action=="over")obj.className=obj.className=="active_off"?"active":"on";if(action=="out")
{for(i=1;i<nTarget;i++)
{obj2=document.getElementById(target+i);if(obj2.className=="active_off")obj2.className="active";}}}
function realPosition(target){var top=target.offsetTop;var left=target.offsetLeft;while(target=target.offsetParent){top+=target.offsetTop;left+=target.offsetLeft;}
return[left,top];}
